{
	"compile": true,
	"tables": [
		{
			"name": "person",
			"output": "csv",
			"rows": 1000,
			"columns": [
				{
					"name": "id",
					"func": "<sid()>"
				},
				{
					"name": "first_name",
					"func": "[first]"
				},
				{
					"name": "last_name",
					"func": "[last]"
				},
				{
					"name": "email",
					"func": "[first].[last]@lazy.com"
				},
				{
					"name": "age",
					"func": "<number(2)>"
				},
				{
					"name": "ballance",
					"func": "<money()>"
				}
			]
		}
	]
}
